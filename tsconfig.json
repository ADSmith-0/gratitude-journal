{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "ES2022",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "alwaysStrict": false,
        "strict": false,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": false,
        "noImplicitReturns": false,
        "noImplicitThis": false
	}
}
